let Nfts = []
function mintNFT(_urge, _skinColor, _height){
    let NFT = {
      "urge":_urge,
      "skinColor":_skinColor,
      "height":_height
    }
    Nfts.push(NFT);
    console.log("Minted" + _urge); 
}
function listNFTs (){
  for (let i =0; i<Nfts.length; i++){
    console.log(Nfts[i]);
  }
}
function getTotalSupply() {
  console.log(Nfts.length);
}
mintNFT("High","Brown","Tall");
mintNFT("High","white","shorty");
listNFTs();
getTotalSupply();
